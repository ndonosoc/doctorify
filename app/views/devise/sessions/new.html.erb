   <div class="container">
      <div class="row mt-3">
         <div class="col-md-4 mx-auto">
            <div class="myform form ">
              <%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
<%= f.input :email,
            required: false,
            autofocus: true,
            input_html: { autocomplete: "email" }, class: "form-control" %>
    <%= f.input :password,
            required: false,
            input_html: { autocomplete: "current-password" }, class: "form-control", placeholder: "******" %>
  </div>

  <div class="form-actions">
        <%= f.button :submit, "Log in", class: "btn btn-primary btn-block" %>
  </div>
  <% end %>
  <div class="container mx-auto justify-content-center">

<%= render "devise/shared/links" %>
  </div>
            </div>
         </div>
      </div>
   </div>

<div class="container mx-auto">
</div>
